{
  "name": "Zapp-Android",
  "version": "1.0.0",
  "description": "Printing Android Apps in a Zapp via [Zapp CMS](https://zapp.applicaster.com)",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  <% if quick_brick_enabled %>"scripts": {
    "quick-brick:start": "cd quick_brick && yarn start",
    "quick-brick:build": "cd quick_brick && yarn build:android && cd .. && yarn quick-brick:copy-bundle",
    "quick-brick:build:debug": "cd quick_brick && yarn build:android:debug && cd .. && yarn quick-brick:copy-bundle",
    "quick-brick:copy-bundle": "cp ./quick_brick/android/main.* ./app/src/mobileGoogle/assets",
    "quick-brick:prepare": "node_modules/.bin/zapplicaster-cli prepare --yarn"
  },<% end %>
  "dependencies": {
    <% if quick_brick_enabled %>"@applicaster/zapplicaster-cli": "<%= quick_brick_version %>",<% end %>
    "react": "16.11.0",
    "react-native": "0.62.2",
    "@react-native-community/cli-platform-android": "^4.7.0",
    "@react-native-community/netinfo": "^5.9.0",
    "@react-native-community/viewpager": "3.3.0",
    "react-native-linear-gradient": "2.5.6",
    "react-native-svg": "9.13.6",
    "@applicaster/react-native-fast-image": "8.3.2-1",
    "react-native-webview": "9.1.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applicaster/Zapp-Android.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/applicaster/Zapp-Android/issues"
  },
  "homepage": "https://github.com/applicaster/Zapp-Android#readme"
}
