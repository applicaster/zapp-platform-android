<resources>

  <string name="app_name"><%= app_name %></string>   <%# Needed for Appoxee - displays at the top of the push notification %>
  <string name="fb_app_id"><%= fb_app_id %></string>
  <string name="ak_login_protocol_scheme">ak<%= fb_app_id %></string>
  <string name="scheme_url_prefix"><%= url_scheme_prefix %></string>
  <string name="app_center_secret"><%= app_center_secret %></string>
  <string name="action_settings">Settings</string>

  <string name="login_redirect_scheme">com.openid.<%= url_scheme_prefix %></string>   <%# Needed for the OpenId Login plugin - added to the manifest of the Pluggable %>

  <%= additional_configuration %>
</resources>
